<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="C971_Isabella_Grigolla.Views.AppSettings">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="ClearSettings" Text="Clear Settings" WidthRequest="150" HorizontalOptions="Center" VerticalOptions="Center" Clicked="ClearSettings_Clicked"></Button>
            <Button x:Name="LoadSampleData" Text="Load Sample Data" WidthRequest="150" HorizontalOptions="Center" VerticalOptions="Center" Clicked="LoadSampleData_Clicked"></Button>
            <Button x:Name="WipeSampleData" Text="Wipe Samle Data" WidthRequest="150" HorizontalOptions="Center" VerticalOptions="Center" Clicked="WipeSampleData_Clicked"></Button>
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>